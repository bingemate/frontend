{
  "name": "binge-mate",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration local",
    "start:dev": "ng serve --configuration development",
    "start:prod": "ng serve --configuration production",
    "build": "ng build --configuration local --progress",
    "build:local": "ng build --configuration local --progress",
    "build:dev": "ng build --configuration development --progress",
    "build:prod": "ng build --configuration production --progress --aot --build-optimizer",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "compodoc:build": "compodoc --theme material -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc --theme material -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc --theme material -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.2",
    "@angular/common": "^15.2.2",
    "@angular/compiler": "^15.2.3",
    "@angular/core": "^15.2.2",
    "@angular/forms": "^15.2.2",
    "@angular/platform-browser": "^15.2.2",
    "@angular/platform-browser-dynamic": "^15.2.2",
    "@angular/router": "^15.2.2",
    "@ngxs/logger-plugin": "^3.7.6",
    "@ngxs/storage-plugin": "^3.7.6",
    "@ngxs/store": "dev",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ng-zorro-antd": "^15.0.3",
    "prettier-eslint": "^15.0.1",
    "rxjs": "~7.8.0",
    "save-dev": "^0.0.1-security",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.3",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.3",
    "@angular/compiler-cli": "^15.2.2",
    "@compodoc/compodoc": "^1.1.19",
    "@ngxs/devtools-plugin": "^3.7.6",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "eslint": "^8.36.0",
    "prettier": "^2.8.4",
    "typescript": "~4.9.4"
  }
}
